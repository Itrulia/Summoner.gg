{
  "paths": {
    "styles": {
      "app": ["app/styles/app.scss"],
      "watch": ["app/styles/**/*.scss"],
      "dist": "dist/styles"
    },
    "typescript": {
      "app": "app/scripts",
      "watch": ["app/scripts/**/*.ts"],
      "dist": "dist/scripts"
    },
    "libraries": {
      "app": {
        "libs": [
          "node_modules/moment/moment.js",
          "node_modules/jquery/dist/jquery.js",
          "node_modules/localforage/dist/localforage.js",
          "node_modules/chart.js/Chart.js",
          "node_modules/lodash/lodash.js",
          "node_modules/angular/angular.js",
          "node_modules/angular-google-analytics/dist/angular-google-analytics.js",
          "node_modules/angular-chart.js/angular-chart.js",
          "node_modules/angular-localforage/dist/angular-localForage.js"
        ]
      },
      "watch": [],
      "dist": "dist/scripts/libs"
    },
    "workers": {
      "app": [
        "app/scripts/workers/**/*.js",
        "node_modules/sw-toolbox/sw-toolbox.js"
      ],
      "watch": ["app/scripts/workers/**/*.js"],
      "dist": "dist/scripts/workers"
    },
    "images": {
      "app": ["app/images/**/*", "!app/images/sprites/**/*"],
      "watch": ["app/images/**/*", "!app/images/sprites/**/*"],
      "dist": "dist/images"
    },
    "sprites": {
      "logos": {
        "app": ["app/images/sprites/logos/*.svg"],
        "dist": "dist/images/sprites/logos"
      },
      "material": {
        "app": ["app/images/sprites/material/*.svg"],
        "dist": "dist/images/sprites/material"
      },
      "watch": ["app/images/sprites/**/*.svg"]
    },
    "index": {
      "app": ["app/*.jade"],
      "watch": ["app/*.jade"],
      "dist": "dist"
    },
    "templates": {
      "app": ["app/templates/**/*.jade"],
      "watch": ["app/templates/**/*.jade"],
      "dist": "dist/scripts"
    }
  }
}